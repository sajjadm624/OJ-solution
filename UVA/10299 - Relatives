#include<bits/stdc++.h>

using namespace std;

int main()
{
    long long int n;
    while(scanf("%lld", &n)==1 && n!=0){
            long long result = n;
            if(n==1){
                printf("0\n");
                continue;
            }

    for(int i=2 ; i*i<=n ; ++i){
        if(n%i==0){
            while(n%i==0){
                n=n/i;
            }
            result = result - result/i;
        }
    }
    if(n>1)
        result = result - result/n;
    printf("%lld\n", result);
    }
return 0;
}
