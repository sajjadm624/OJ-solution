#include<stdio.h>
#include<string.h>

int main()
{
    char str[1000] , c;
    int i , j , len;

       while(scanf("%s", &str)==1)
       {
          c = getchar();
          len = strlen(str);
          for( i=len-1 ; i>=0 ; i-- )
          {
              printf("%c", str[i]);
          }
          putchar(c);
       }
    return 0;
}
